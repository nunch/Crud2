

@using PerennisationSPI.Models
@{
    Layout = null;
FichierSet fichierSet = (FichierSet) ViewBag.fichierSet;
    
        bool useChosen_on_fk_fk_FichierSet_0 = true;
    
}


<form method="post" id="modifyData-form" action="/FichierSet/update">
    <fieldset>
        <legend>Modifier FichierSet</legend>
                <input type="hidden" id="Id_Fichier" name="Id_Fichier" value="@fichierSet.Id_Fichier" />
                <div class="form-group" type="requiredInput">
                    <label class="control-label">NomFichier</label>
                            <input type="text" maxlength="0" id="NomFichier" name="NomFichier" class="form-control" required=required  value="@fichierSet.NomFichier" />
                    <div id="NomFichier-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir NomFichier</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">NbSucces</label>
                            <input type="number" id="NbSucces" name="NbSucces" class="form-control" required=required  value="@fichierSet.NbSucces" />
                    <div id="NbSucces-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir NbSucces</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">NbErreur</label>
                            <input type="number" id="NbErreur" name="NbErreur" class="form-control" required=required  value="@fichierSet.NbErreur" />
                    <div id="NbErreur-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir NbErreur</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">IsSuppr</label>
                            <input type="number" id="IsSuppr" name="IsSuppr" class="form-control" required=required  value="@fichierSet.IsSuppr" />
                    <div id="IsSuppr-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir IsSuppr</p>
                    </div>
                </div>
                
                    @if(useChosen_on_fk_fk_FichierSet_0)
                    {
                    <div class="form-group" id="fk-0-div">
                        <input type="hidden" id="Id_RapportIntegration-object" value="@fichierSet.Id_RapportIntegration" />
                        <label class="control-label">Id_RapportIntegration</label>
                        <select data-placeholder="Choisir Id_RapportIntegration" class="chosen-select" id="Id_RapportIntegration" name="Id_RapportIntegration" required=required>
                            <option disabled selected>Choisir Id_RapportIntegration</option>
                            <option value="">Aucun</option>
                        </select>
                        <div id="fk-0-div-collapse-div" class="collapse">
                            <p class="help-block">Veuillez choisir Id_RapportIntegration</p>
                        </div>
                    </div>
                    }else
                    {
                    <div id="fk-0-div" class="form-group">
                        <label class="control-label">Id_RapportIntegration</label>
                        <input type="hidden" name="Id_RapportIntegration" id="Id_RapportIntegration" value="@fichierSet.Id_RapportIntegration" required=required />
                        <div class="row">
                            <div class="col-xs-1">
                                <button id="btnResetFilters-fk-0" class="btn refresh" data-rel="tooltip" data-toggle="tooltip" data-original-title="R&eacute;initialiser les filtres">
                                    <i class="fa fa-refresh Icon"></i>
                                </button>
                            </div>
                            <div class="col-xs-4 ">
                                <div id="tab_filter-fk-0" class="customFilter">
                                    <label>
                                        <input id="customFilterInput-fk-0" type="search" class="form-control input-sm" placeholder="Rechercher..." aria-controls="fk-0-table">
                                    </label>
                                </div>
                            </div>
                        </div>
                        <table id="fk-0-table">
                            <tfoot>
                                <tr>
                                        <th>Id_RapportIntegration</th>
                                        <th>NbFichiers</th>
                                        <th>NbTotalSucces</th>
                                        <th>NbTotalErreur</th>
                                        <th>DateImport</th>
                                        <th>IsSuppr</th>
                                </tr>
                            </tfoot>
                        </table>
                        <div id="fk-0-div-collapse-div" class="collapse">
                            <p class="help-block">Veuillez choisir Id_RapportIntegration</p>
                        </div>
                    </div>
                    }
                
                <div class="row">
            <div class="col-md-2 col-sm-2 col-xs-4 col-xs-offset-4">
                <a href="#" id="form-submit" class="btn btn-sm animated-button victoria-cancel">Modifier</a>
            </div>
            <div class="col-md-2 col-sm-2 col-xs-4">
                <a href="#" id="form-cancel" class="btn btn-sm animated-button victoria-four ">Annuler</a>
            </div>
        </div>

</fieldset>

</form>

<script src="/Scripts/Ajax/FichierSet/modify.js"></script>
