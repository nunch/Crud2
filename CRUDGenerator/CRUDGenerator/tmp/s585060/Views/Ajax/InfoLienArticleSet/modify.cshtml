                    <div id="fk-0-div" class="form-group">
                        <label class="control-label">Id_LienArticle</label>
                        <input type="hidden" name="Id_LienArticle" id="Id_LienArticle" value="@infoLienArticleSet.Id_LienArticle" required=required />
                        <div class="row">
                            <div class="col-xs-1">
                                <button id="btnResetFilters-fk-0" class="btn refresh" data-rel="tooltip" data-toggle="tooltip" data-original-title="R&eacute;initialiser les filtres">
