

@using PerennisationSPI.Models
@{
    Layout = null;
InfoLienArticleSet infoLienArticleSet = (InfoLienArticleSet) ViewBag.infoLienArticleSet;
    
        bool useChosen_on_fk_fk_InfoLienArticleSet_0 = true;
    
}


<form method="post" id="modifyData-form" action="/InfoLienArticleSet/update">
