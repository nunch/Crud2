                        <label class="control-label">Id_LienArticle</label>
                        <select data-placeholder="Choisir Id_LienArticle" class="chosen-select" id="Id_LienArticle" name="Id_LienArticle" required=required>
                            <option disabled selected>Choisir Id_LienArticle</option>
                            <option value="">Aucun</option>
