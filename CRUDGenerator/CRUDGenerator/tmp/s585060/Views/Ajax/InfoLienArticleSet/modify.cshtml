                        <p class="help-block">Veuillez choisir DateDebut</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">DateFin</label>
                            <input type="text" id="DateFin" class="datetimepicker form-control" name="DateFin"   value="@infoLienArticleSet.DateFin" />
                    <div id="DateFin-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir DateFin</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Champ1</label>
