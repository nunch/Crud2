                    <div id="Champ1-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir Champ1</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Champ2</label>
                            <input type="text" maxlength="0" id="Champ2" name="Champ2" class="form-control"   value="@infoLienArticleSet.Champ2" />
