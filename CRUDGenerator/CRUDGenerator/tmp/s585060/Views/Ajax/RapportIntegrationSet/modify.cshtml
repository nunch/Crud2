

@using PerennisationSPI.Models
@{
    Layout = null;
RapportIntegrationSet rapportIntegrationSet = (RapportIntegrationSet) ViewBag.rapportIntegrationSet;
}


<form method="post" id="modifyData-form" action="/RapportIntegrationSet/update">
    <fieldset>
        <legend>Modifier RapportIntegrationSet</legend>
                <input type="hidden" id="Id_RapportIntegration" name="Id_RapportIntegration" value="@rapportIntegrationSet.Id_RapportIntegration" />
                <div class="form-group" type="requiredInput">
                    <label class="control-label">NbFichiers</label>
                            <input type="number" id="NbFichiers" name="NbFichiers" class="form-control" required=required  value="@rapportIntegrationSet.NbFichiers" />
                    <div id="NbFichiers-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir NbFichiers</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">NbTotalSucces</label>
                            <input type="number" id="NbTotalSucces" name="NbTotalSucces" class="form-control" required=required  value="@rapportIntegrationSet.NbTotalSucces" />
                    <div id="NbTotalSucces-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir NbTotalSucces</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">NbTotalErreur</label>
                            <input type="number" id="NbTotalErreur" name="NbTotalErreur" class="form-control" required=required  value="@rapportIntegrationSet.NbTotalErreur" />
                    <div id="NbTotalErreur-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir NbTotalErreur</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">DateImport</label>
                            <input type="text" id="DateImport" class="datetimepicker form-control" name="DateImport" required=required  value="@rapportIntegrationSet.DateImport" />
                    <div id="DateImport-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir DateImport</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">IsSuppr</label>
                            <input type="number" id="IsSuppr" name="IsSuppr" class="form-control" required=required  value="@rapportIntegrationSet.IsSuppr" />
                    <div id="IsSuppr-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir IsSuppr</p>
                    </div>
                </div>
                <div class="row">
            <div class="col-md-2 col-sm-2 col-xs-4 col-xs-offset-4">
                <a href="#" id="form-submit" class="btn btn-sm animated-button victoria-cancel">Modifier</a>
            </div>
            <div class="col-md-2 col-sm-2 col-xs-4">
                <a href="#" id="form-cancel" class="btn btn-sm animated-button victoria-four ">Annuler</a>
            </div>
        </div>

</fieldset>

</form>

<script src="/Scripts/Ajax/RapportIntegrationSet/modify.js"></script>
