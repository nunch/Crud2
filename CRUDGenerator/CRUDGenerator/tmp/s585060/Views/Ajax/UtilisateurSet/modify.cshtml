

@using PerennisationSPI.Models
@{
    Layout = null;
UtilisateurSet utilisateurSet = (UtilisateurSet) ViewBag.utilisateurSet;
    
        bool useChosen_on_fk_fk_UtilisateurSet_0 = true;
    
}


<form method="post" id="modifyData-form" action="/UtilisateurSet/update">
    <fieldset>
        <legend>Modifier UtilisateurSet</legend>
                <input type="hidden" id="Id_Utilisateur" name="Id_Utilisateur" value="@utilisateurSet.Id_Utilisateur" />
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Nom</label>
                            <input type="text" maxlength="0" id="Nom" name="Nom" class="form-control" required=required  value="@utilisateurSet.Nom" />
                    <div id="Nom-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir Nom</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Prenom</label>
                            <input type="text" maxlength="0" id="Prenom" name="Prenom" class="form-control" required=required  value="@utilisateurSet.Prenom" />
                    <div id="Prenom-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir Prenom</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Matricule</label>
                            <input type="text" maxlength="0" id="Matricule" name="Matricule" class="form-control" required=required  value="@utilisateurSet.Matricule" />
                    <div id="Matricule-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir Matricule</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Mail</label>
                            <input type="text" maxlength="0" id="Mail" name="Mail" class="form-control" required=required  value="@utilisateurSet.Mail" />
                    <div id="Mail-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir Mail</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">Role</label>
                            <input type="number" id="Role" name="Role" class="form-control" required=required  value="@utilisateurSet.Role" />
                    <div id="Role-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir Role</p>
                    </div>
                </div>
                <div class="form-group" type="requiredInput">
                    <label class="control-label">IsSuppr</label>
                            <input type="number" id="IsSuppr" name="IsSuppr" class="form-control" required=required  value="@utilisateurSet.IsSuppr" />
                    <div id="IsSuppr-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir IsSuppr</p>
                    </div>
                </div>
                
                    @if(useChosen_on_fk_fk_UtilisateurSet_0)
                    {
                    <div class="form-group" id="fk-0-div">
                        <input type="hidden" id="Id_Entite-object" value="@utilisateurSet.Id_Entite" />
                        <label class="control-label">Id_Entite</label>
                        <select data-placeholder="Choisir Id_Entite" class="chosen-select" id="Id_Entite" name="Id_Entite" >
                            <option disabled selected>Choisir Id_Entite</option>
                            <option value="">Aucun</option>
                        </select>
                        <div id="fk-0-div-collapse-div" class="collapse">
                            <p class="help-block">Veuillez choisir Id_Entite</p>
                        </div>
                    </div>
                    }else
                    {
                    <div id="fk-0-div" class="form-group">
                        <label class="control-label">Id_Entite</label>
                        <input type="hidden" name="Id_Entite" id="Id_Entite" value="@utilisateurSet.Id_Entite"  />
                        <div class="row">
                            <div class="col-xs-1">
                                <button id="btnResetFilters-fk-0" class="btn refresh" data-rel="tooltip" data-toggle="tooltip" data-original-title="R&eacute;initialiser les filtres">
                                    <i class="fa fa-refresh Icon"></i>
                                </button>
                            </div>
                            <div class="col-xs-4 ">
                                <div id="tab_filter-fk-0" class="customFilter">
                                    <label>
                                        <input id="customFilterInput-fk-0" type="search" class="form-control input-sm" placeholder="Rechercher..." aria-controls="fk-0-table">
                                    </label>
                                </div>
                            </div>
                        </div>
                        <table id="fk-0-table">
                            <tfoot>
                                <tr>
                                        <th>IsSuppr</th>
                                        <th>Id_Entite</th>
                                        <th>Nom</th>
                                        <th>Description</th>
                                </tr>
                            </tfoot>
                        </table>
                        <div id="fk-0-div-collapse-div" class="collapse">
                            <p class="help-block">Veuillez choisir Id_Entite</p>
                        </div>
                    </div>
                    }
                
                <div class="form-group" type="requiredInput">
                    <label class="control-label">DateDerniereConnexion</label>
                            <input type="text" id="DateDerniereConnexion" class="datetimepicker form-control" name="DateDerniereConnexion"   value="@utilisateurSet.DateDerniereConnexion" />
                    <div id="DateDerniereConnexion-collapse-div" class="collapse">
                        <p class="help-block">Veuillez choisir DateDerniereConnexion</p>
                    </div>
                </div>
                <div class="row">
            <div class="col-md-2 col-sm-2 col-xs-4 col-xs-offset-4">
                <a href="#" id="form-submit" class="btn btn-sm animated-button victoria-cancel">Modifier</a>
            </div>
            <div class="col-md-2 col-sm-2 col-xs-4">
                <a href="#" id="form-cancel" class="btn btn-sm animated-button victoria-four ">Annuler</a>
            </div>
        </div>

</fieldset>

</form>

<script src="/Scripts/Ajax/UtilisateurSet/modify.js"></script>
